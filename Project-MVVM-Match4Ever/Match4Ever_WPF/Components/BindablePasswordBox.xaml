<UserControl x:Class="Match4Ever_WPF.Components.BindablePasswordBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Match4Ever_WPF.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">


    <!--Geïmplementeerd uit het voorbeeld van Kilian!-->
    <PasswordBox x:Name="MvvmPasswordBox" PasswordChanged="MvvmPasswordBox_PasswordChanged" Style="{StaticResource RoundPasswordBox}"/>
    
    
</UserControl>
